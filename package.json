{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "next dev",
    "hcm:run": "hcm 'src/**/*.module.css'",
    "hcm:watch": "yarn hcm:run -w",
    "dev": "concurrently \"npm:serve\" \"npm:hcm:watch\" -c blue,green",
    "prebuild": "yarn hcm:run",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "format": "run-s format:prettier format:eslint",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "typecheck": "tsc",
    "ncu:dev": "ncu -u --dep dev",
    "ncu:prod": "ncu -u --dep prod"
  },
  "dependencies": {
    "next": "13.4.4",
    "next-sitemap": "4.1.3",
    "next-themes": "0.2.1",
    "open-color": "git+https://github.com/y-temp4/open-color.git",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "1.13.1"
  },
  "devDependencies": {
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.2.5",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "autoprefixer": "10.4.14",
    "concurrently": "8.1.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tailwindcss": "3.12.1",
    "happy-css-modules": "2.1.1",
    "npm-check-updates": "16.10.12",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.24",
    "postcss-nesting": "11.2.2",
    "prettier": "2.8.8",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  }
}
