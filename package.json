{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "next dev",
    "hcm:run": "hcm 'src/**/*.module.css'",
    "hcm:watch": "pnpm hcm:run -w",
    "dev": "concurrently \"npm:serve\" \"npm:hcm:watch\" -c blue,green",
    "prebuild": "pnpm hcm:run",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "run-p -lcn lint:*",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "lint:typecheck": "tsc",
    "format": "run-s format:prettier format:eslint",
    "format:eslint": "pnpm lint:eslint --fix",
    "format:prettier": "pnpm lint:prettier --write",
    "ncu:dev": "ncu -u --dep dev",
    "ncu:prod": "ncu -u --dep prod"
  },
  "dependencies": {
    "next": "14.2.5",
    "next-sitemap": "4.2.3",
    "next-themes": "0.3.0",
    "nuqs": "1.17.6",
    "open-color": "git+https://github.com/y-temp4/open-color.git",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "2.4.0"
  },
  "devDependencies": {
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.0.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-tailwindcss": "3.17.4",
    "happy-css-modules": "3.1.1",
    "npm-check-updates": "17.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.39",
    "postcss-nesting": "12.1.5",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.7",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
